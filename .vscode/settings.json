{
	"editor.fontFamily": "'Fira Code Retina', 'Jetbrains Mono', 'Cascadia Code', Consolas, 'Courier New', monospace",
	"editor.fontLigatures": true,
	"editor.fontSize": 17,
	"editor.linkedEditing": true,
	"editor.lightbulb.enabled": true,
	"editor.formatOnSave": true,
	"editor.stickyScroll.enabled": true,
	"clangd.checkUpdates": false,
	"clangd.serverCompletionRanking": true,
	"clangd.path": "/usr/bin/clangd-15",
	"clangd.arguments": [
		"--compile-commands-dir=${workspaceFolder}/build",
		"--pch-storage=memory",
		"--clang-tidy=true",
		"--all-scopes-completion=true",
		"--background-index=true",
		"--inlay-hints=true",
		"--pretty",
	],
	"editor.semanticHighlighting.enabled": true,
	"editor.renderLineHighlight": "all",
	"editor.renderWhitespace": "boundary",
	"editor.minimap.renderCharacters": true,
	"terminal.integrated.fontFamily": "Hasklug NF",
	"C_Cpp.intelliSenseEngine": "disabled",
	"C_Cpp.intelliSenseEngineFallback": "disabled",
	"C_Cpp.clang_format_path": "clang-format-15",
	"C_Cpp.clang_format_fallbackStyle": "Visual Studio",
	"C_Cpp.autocomplete": "default",
	"C_Cpp.codeFolding": "enabled",
	"C_Cpp.enhancedColorization": "enabled",
	"C_Cpp.autocompleteAddParentheses": true,
	"C_Cpp.loggingLevel": "Information",
	"C_Cpp.autoAddFileAssociations": true,
	"C_Cpp.codeAnalysis.clangTidy.enabled": true,
	"C_Cpp.codeAnalysis.clangTidy.path": "/usr/bin/clang-tidy-15",
	"C_Cpp.codeAnalysis.runAutomatically": true,
	"editor.semanticTokenColorCustomizations": {
		"rules": {
			"enum": {
				"fontStyle": "bold"
			},
			"enumMember": {
				"foreground": "#a0cd00",
				"fontStyle": "italic bold"
			},
			"macro": {
				"foreground": "#2cac61",
				"fontStyle": "bold underline"
			},
			"function": {
				"fontStyle": "bold"
			},
			"variable": {
				"fontStyle": "italic"
			},
			"property": {
				"fontStyle": "italic underline",
				"foreground": "#c943e1"
			}
		}
	},
	"workbench.colorTheme": "Snazzy Light",
	"oneDarkPro.vivid": true,
	"oneDarkPro.bold": true,
	"files.associations": {
		"array": "c",
		"*.tcc": "c",
		"bitset": "c",
		"functional": "c",
		"mutex": "c",
		"tuple": "c",
		"variant": "c",
		"chrono": "c",
		"__config": "c",
		"__tuple": "c",
		"ios": "c",
		"span": "c",
		"string": "c",
		"string_view": "c",
		"vector": "c",
		"kernel.h": "c",
		"init.h": "c",
		"module.h": "c",
		"virtio_dma_buf.h": "c",
		"virtio_mmio.h": "c",
		"virtio_input.h": "c",
		"virtio_gpio.h": "c",
		"virtio_console.h": "c",
		"virtio.h": "c",
		"virtio_i2c.h": "c",
		"vringh.h": "c",
		"virtio_ring.h": "c",
		"virtio_config.h": "c",
		"domain.h": "c",
		"globals.h": "c",
		"master.h": "c"
	},
	"cSpell.enabled": true,
	"cSpell.words": [
		"absl",
		"ecrt",
		"ictrl",
		"NOLINTNEXTLINE",
		"PDOS",
		"preceeding",
		"rclpy",
		"rxpdo",
		"traj",
		"unconfigured",
		"urdf",
		"waitables"
	],
	"ros.distro": "humble",
	"ros.rosSetupScript": "/opt/humble/setup.sh",
	"python.analysis.autoImportCompletions": true,
	"python.autoComplete.extraPaths": [
		"/opt/ros/humble/lib/python3.10/site-packages",
		"/opt/ros/humble/local/lib/python3.10/dist-packages",
	],
	"python.analysis.extraPaths": [
		"/opt/ros/humble/lib/python3.10/site-packages",
		"/opt/ros/humble/local/lib/python3.10/dist-packages",
	],
	"python.linting.mypyEnabled": true,
	"python.linting.enabled": true,
	"python.linting.mypyPath": "mypy",
	"python.formatting.yapfPath": "yapf",
	"python.formatting.provider": "yapf",
	"python.linting.pylintEnabled": false,
	"python.linting.pylintPath": "pylint",
	"cmake.sourceDirectory": "${workspaceFolder}/icnc_control_node",
}